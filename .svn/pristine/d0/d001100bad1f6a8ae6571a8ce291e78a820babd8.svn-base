using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SVC_1.entidades
{
    public class CotacaoPreco
    {
        int idCotacaoPreco;
        DateTime dataCricao;
        List<ItemCotacaoPreco> listaItens;

        public int IdCotacaoPreco
        {
            get { return idCotacaoPreco; }
            set { idCotacaoPreco = value; }
        }

        public DateTime DataCriacao
        {
            get { return dataCricao; }
            set { dataCricao = value; }
        }

        public List<ItemCotacaoPreco> ListaItens
        {
            get { return listaItens; }
            set { listaItens = value; }
        }

    }
}
