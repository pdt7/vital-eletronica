using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SVC_1.entidades
{
    public class Obra
    {
        public int idObra;
        public string codigo;
        public string local;
        public string rua;
        public string numero;
        public string bairro;
        public string cidade;
        public string estado;
        public string cep;
        public string cnpj;
        public string inscricaoEstatual;

        public int IdObra
        {
            get { return idObra; }
            set { idObra = value; }
        }

        public string Codigo
        {
            get { return codigo; }
            set { codigo = value; }
        }

        public string Local
        {
            get { return local; }
            set { local = value; }
        }

        public string Rua
        {
            get { return rua; }
            set { rua = value; }
        }

        public string Numero
        {
            get { return numero; }
            set { numero = value; }
        }

        public string Bairro
        {
            get { return bairro; }
            set { bairro = value; }
        }

        public string Cidade
        {
            get { return cidade; }
            set { cidade = value; }
        }

        public string Estado
        {
            get { return estado; }
            set { estado = value; }
        }

        public string Cep
        {
            get { return cep; }
            set { cep = value; }
        }

        public string CNPJ
        {
            get { return cnpj; }
            set { cnpj = value; }
        }

        public string InscricaoEstadual
        {
            get { return inscricaoEstatual; }
            set { inscricaoEstatual = value; }
        }

    }
}
